#include <bits/stdc++.h>
using namespace std;
 void print(int n){
     //base case
     if(n==0){
     return ;
 }
    cout << n << endl;
    print(n-1);
 }

int main() {
    int n;
    cin>>n;
   print(n);
    return 0;
}

head recurrsion 
#include <bits/stdc++.h>
using namespace std;
 void print(int n){
     //base case
     if(n==0){
     return ;
 }
 //recursive relation
     print(n-1);
    cout << n << endl;

 }

int main() {
    int n;
    cin>>n;
   print(n);
    return 0;
}
